{{outlet}}
{{#each model.rider as |rider|}}
{{update-status rider=rider updateStatus1="updateStatus2"}}
<h1>This is {{rider.name}}'s Dashboard</h1>
{{/each}}
<table class="table table-hover">
  <thead>
    <tr>
      <th>Clients Destination</th>
      <th>Clients Name</th>
      <th>Clients email</th>
    </tr>
  </thead>
  <tbody>
    {{#each model.rider as |rider|}}
      {{#each rider.rides as |ride|}}
        <tr>
          <td>{{ride.to}}</td>
          <td>{{ride.user.name}}</td>
          <td>{{ride.user.email}}</td>
        </tr>
      {{/each}}
    {{/each}}
  </tbody>
</table>
